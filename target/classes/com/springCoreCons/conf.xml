<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:p="http://www.springframework.org/schema/p"
	xsi:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 <bean class="com.springCoreCons.Certificate" name="cer">
 <constructor-arg value="Java"/>
 </bean>
<bean class="com.springCoreCons.Person"   name="per">

<constructor-arg  value="Abubakar"/>
<constructor-arg  value="12"/>
<constructor-arg   ref="cer"/>
</bean>
</beans>