<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<context:annotation-config/>
	<bean class="com.autowiredByAnno.Address" name="address">
		<property name="town" value="bgn" />
		<property name="city" value="Santkabir" />
	</bean>
	<bean class="com.autowiredByAnno.Address" name="ss">
		<property name="town" value="bgn" />
		<property name="city" value="basti" />
		</bean>
	<bean class="com.autowiredByAnno.Student" name="student" />




</beans>